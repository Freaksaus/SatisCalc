@model IEnumerable<SatisCalc.Models.Item>;
@{
    ViewData["Title"] = "Item";
}

<h2>Items</h2>

<div class="row">
    <div class="col-md-12 pull-right">
        <a asp-action="Add">Add</a>
    </div>
</div>

@foreach (var item in Model)
{
    <div class="row">
        <div class="col-md-8">
            @item.Name
        </div>
        <div class="col-md-4">
            @item.Category
        </div>
    </div>
}